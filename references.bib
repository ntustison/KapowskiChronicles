	Journal = {Cereb Cortex},
	Journal-Full = {Cerebral cortex (New York, N.Y. : 1991)},
	Mesh = {Adult; Algorithms; Brain Mapping; Cerebral Cortex; Female; Humans; Magnetic Resonance Imaging; Male; Models, Neurological; Nerve Net; Young Adult},
