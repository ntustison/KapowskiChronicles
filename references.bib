	Doi = {10.1016/j.jns.2008.12.034},
	Journal = {J Neurol Sci},
	Journal-Full = {Journal of the neurological sciences},
	Mesh = {Adult; Atrophy; Automatic Data Processing; Brain; Case-Control Studies; Cerebellum; Cerebral Cortex; Demyelinating Diseases; Disease Progression; Female; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Multiple Sclerosis, Chronic Progressive; Multiple Sclerosis, Relapsing-Remitting; Nerve Fibers, Myelinated; Organ Size; Severity of Illness Index; Software},
