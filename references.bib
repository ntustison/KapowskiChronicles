	Journal = {J Behav Brain Sci},
	Journal-Full = {Journal of behavioral and brain science},
	Keywords = {Brain; Cortex; Gender Dysphoria; Gender Identity Disorder; Gender Incongruence; Gender Nonconformity; MRI; Transgender},
	Month = {Aug},
	Number = {3},
