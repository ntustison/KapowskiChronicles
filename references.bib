	Journal-Full = {NeuroImage},
	Mesh = {Adolescent; Aging; Cerebral Cortex; Child; Female; Humans; Magnetic Resonance Imaging; Reproducibility of Results; Scoliosis; Sensitivity and Specificity},
	Month = {Jan},
	Number = {2},
	Pages = {935-42},
	Pmid = {21872666},
